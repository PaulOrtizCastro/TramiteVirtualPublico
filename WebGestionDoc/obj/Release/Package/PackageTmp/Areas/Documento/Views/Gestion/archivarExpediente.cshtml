@using (Html.BeginForm("archivarExpediente", "Gestion", FormMethod.Post, new { @id = "frmArchivarExpediente", @class = "", @enctype = "multipart/form-data" }))
{
    <div class="panel-body">
        <div class="form-group row">
            <div class="col-sm-2">
                <label>
                    Observaciones:</label>
            </div>
            <div class="col-sm-10">
                <textarea id="archivarExpe_taComentarios" rows="6" cols="100"></textarea>
            </div>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-12" style="text-align: right">
            <button id="archivarExpe_btnCancelar" class="btn btn-default" type="button">
                <i class="clip-close" />Cancelar
            </button>
            <button id="archivarExpe_btnGuardar" class="btn btn-primary" type="button"
                data-callback='archivarExpe_grabar()'>
                Archivar <i class="fa fa-briefcase"></i>
            </button>
        </div>
    </div>
    <div style="display: none">
        <input type="hidden" id="archivarExpe_grabaridExpe" value= @ViewBag.ID_EXPE />
        <input type="hidden" id="archivarExpe_grabaridMovi" value= @ViewBag.ID_MOVI />
        <input type="hidden" id="archivarExpe_grabaridAccion" value=@ViewBag.ID_ACCION />
        <input type="hidden" id="archivarExpe_grabaridDoc" value= @ViewBag.ID_DOC />
        <input type="hidden" id="archivarExpe_grabaridTipDoc" value= @ViewBag.ID_TIP_DOC />
        <input type="hidden" id="archivarExpe_grabaridSub" value= @ViewBag.ID_SUB />
        <input type="hidden" id="archivarExpe_grabaridSubOfi" value= @ViewBag.ID_SUBOFI />
        <input type="hidden" id="archivarExpe_grabaridPersona" value= @ViewBag.ID_PERSONA />
        <input type="hidden" id="archivarExpe_callback" value="@ViewBag.strCallBack" />
    </div>
    @Scripts.Render("~/dist/src/app.Registro.regArchivarDocPublico.chunk")
}